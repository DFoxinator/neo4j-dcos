{
   "id":"neo4j-lb",
   "cpus":1,
   "mem":1024,
   "instances":1,
   "container":{
      "type":"DOCKER",
      "docker":{
         "image":"unterstein/dcos-neo4j-proxy:latest",
         "forcePullImage": true,
          "network":"BRIDGE",
          "portMappings":[
            {
              "hostPort":7474,
              "containerPort":7474,
              "protocol":"tcp"
            },
            {
              "hostPort":80,
              "containerPort":80,
              "protocol":"tcp"
            }
          ]
      }
   },
   "acceptedResourceRoles":["slave_public"]
}